generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id           Int       @id @default(autoincrement())
  email        String    @unique
  password     String
  refreshToken String?
  rpcUrl       String?
  wssRpcUrl    String?
  devWallet    String?
  isConfirmed  Boolean   @default(false)
  isSettingConfirmed  Boolean   @default(false)
  createdAt    DateTime  @default(now())
  updatedAt    DateTime  @updatedAt
}

